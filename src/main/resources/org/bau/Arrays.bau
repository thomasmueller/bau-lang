module org.bau.Arrays

fun reverse(buff T[], first 0..buff.len, last 0..buff.len)
    while first < last
        temp : buff[first]!
        buff[first]! = buff[last]!
        buff[last]! = temp
        f := first + 1
        break f >= buff.len
        first = f
        l := last - 1
        break l < 0
        break l >= buff.len
        last = l