module org.bau.Env

fun argCount() int
    native(```
        return __argc;
        ```)
    return 0

fun arg(index int) i8[]
    if index < 0 or index >= argCount()
        return new(i8[], 0)
    len := 0
    native(```
        #include <string.h>
        len = strlen(__argv[index]);
        ```)
    result : new(i8[], len)
    native(```
        strncpy((char*) result->data, __argv[index], len);
        ```)
    return result
    

