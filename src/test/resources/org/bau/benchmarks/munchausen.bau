import org.bau.Math

fun main()
    for i := until(LIMIT)
        if isMunchausen(i)
            println(i)

fun isMunchausen(number int) int
    n := number
    total := 0
    while n > 0
        digit : n % 10
        total += cache[digit]
        if total > number
            return 0
        n /= 10
    return total = number

fun getCache() const int[]
    result : int[10]
    for i := range(1, 10)
        result[i] = int(Math.pow(i, i))
    return result

LIMIT : 440_000_000
cache : getCache()
