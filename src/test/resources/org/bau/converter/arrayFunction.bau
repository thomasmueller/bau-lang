fun fill(data int[])
  if data.len <= 0
    return
  i := 0..data.len
  while 1 = 1
    data[i]! = i
    next : i + 1
    break next >= data.len
    i = next

fun arrayOf(T type, entries T..) T[]
    return entries

fun i8[] len() int
  return this.len

fun sum(a int, b int) int
