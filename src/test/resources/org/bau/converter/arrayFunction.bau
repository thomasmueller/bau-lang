fun fill(data int[])
  if data.len <= 0
    return
  i := 0..data.len
  while 1 = 1
    data[i]! = i
    next : i + 1
    break next >= data.len
    i = next

fun test()
  data : new(int[], 16)
  fill(data)
  for i := until(data.len)
    println(data[i]!)

test()

