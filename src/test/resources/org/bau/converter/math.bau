import org.bau.Math

fun operations(a int, b int)
    if b <> 0
        println(a '  / ' b ' = ' (a / b))
        println(a '  % ' b ' = ' (a % b))
    println(a ' >> ' b ' = ' (a >> b))
    println(a ' << ' b ' = ' (a << b))

fun main()
    operations(0, 0)
    operations(1, 0)
    operations(-1, 0)
    operations(3, -50)

    x := -1.0
    x = -1
    while x <= 1
        println('signum ' x ' = ' Math.signum(x))
        println('abs    ' x ' = ' Math.abs(x))
        println('floor  ' x ' = ' Math.floor(x))
        println('ceil   ' x ' = ' Math.ceil(x))
        println('round  ' x ' = ' Math.round(x))
        println('exp    ' x ' = ' Math.exp(x))
        println('log    ' x ' = ' Math.log(x))
        println('log10  ' x ' = ' Math.log10(x))
        println('sqrt   ' x ' = ' Math.sqrt(x))
        println('sin    ' x ' = ' Math.sin(x))
        println('cos    ' x ' = ' Math.cos(x))
        println('tan    ' x ' = ' Math.tan(x))
        println('atan   ' x ' = ' Math.atan(x))
        println('asin   ' x ' = ' Math.asin(x))
        println('acos   ' x ' = ' Math.acos(x))
        x += 0.5

    x = -1
    while x <= 1
        println('isNotANumber   ' x ' = ' Math.isNotANumber(x))
        println('isNegativeZero ' x ' = ' Math.isNegativeZero(x))
        println('convertDoubleToLongBits ' x ' = ' Math.convertDoubleToLongBits(x))
        println('convertLongBitsToDouble ' x ' = ' Math.convertLongBitsToDouble(4602678819172646912))
        x += 0.5

    x = -1
    while x <= 1
        y := -1.0
        while y <= 1
            println('pow ' Math.pow(x, y))
            println('min ' Math.minFloat(x, y))
            println('max ' Math.maxFloat(x, y))
            y += 0.5
        x += 0.5

