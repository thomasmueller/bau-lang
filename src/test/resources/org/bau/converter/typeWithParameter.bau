type List(T)
  array T[]
  size int

fun List(T) add(x T)
  if size >= array.len
    n : new(T[], array.len * 2)
    array = n
  array[size] = x
  size += 1

fun test()
  intList := new(List(int))
  intList.array = new(int[], 4)
  intList.add(10)
  intList.add(20)
  println(intList.array[0])
  println(intList.array[1])

test()
